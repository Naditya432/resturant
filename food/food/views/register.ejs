    <div class="container wrapper">
        <div class="contact-form">
            <h3>Register Here!</h3>
            <form action="/participants" method="POST">
                <div>
                    <label for="FIRSTNAME">FIRSTNAME : </label>
                    <input type="text" class="input" placeholder="FIRST NAME" name="fname" required>
                </div>
                <div>
                    <label for="LASTNAME">LASTNAME : </label>
                    <input type="text" class="input" placeholder="LAST NAME" name="lname" required>
                </div>
                <div>
                    <label for="EMAIL">EMAIL : </label>
                    <input type="email" class="input" placeholder="EMAIL" name="email" required>
                </div>
              
                <div>
                    <select name="stream" id="input" class="sel">
                        <option value="STREAM" disabled>STREAM</option>
                       
                        <option value="TEA">TEA</option>
                          <option value="COFEE">COFEE</option>
                        <option value="IDLI">IDLI</option>
                          <option value="VADA">VADA</option>
                        <option value="MASALADOSA">MASALADOSA</option>
                        <option value="SOUTH-MEALS">SOUTH MEALS</option>
                        <option value="ROTI CURRY">ROTI CURRY</option>
                        <option value="NOTH-MEALS">NORTH MEALS</option>
                          <option value="GOBI">GOBI</option>
                        <option value="FRIED-RICE">FRIED RICE</option>
                        <option value="ICE CREAM">ICE CREAM</option>
                    </select>
                    <select name="classes" id="input" class="sel" name="clg">
                        <option value="CLASS" disabled> DELIVERY-LOCATION</option>
                        <option value="KALYAN NAGAR">KALYAN NAGAR</option>
                        <option value="BANASWADI">BANASWADI</option>
                        <option value="K.R.PURAM">K.R.PURAM</option>
                        <option value="RAMURTHYNAGAR">RAMURTHYNAGAR</option>
                        <option value="HEBBAL">HEBBAL</option>
                        <option value="VIDAYARANYAPURA">VIDAYARANYAPURA</option>
                        <option value="YELAHANKA">YELAHANKA</option>
                    </select>
                </div>
                <div>
                    <label for="checkbox">I agree to the terms and conditions</label>
                    <input id="checkbox" type="checkbox" name="checkbox" required="checkbox">
                </div>
                <div class="submit">
                    <input type="submit" class="sub">
                </div>
            </form>
        </div>
